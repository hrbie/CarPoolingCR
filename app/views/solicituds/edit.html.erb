<h1>Editing article</h1>

<%= form_for :solicitud, url: usuario_viaje_solicitud_path(@usuario,@viaje,@solicitud), method: :patch do |f| %>

    <p>
      <%= f.label :title %><br>
      <%= f.text_field :comentario %>
    </p>

    <p>
      <%= f.label :text %><br>
      <%= f.text_area :estado %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>