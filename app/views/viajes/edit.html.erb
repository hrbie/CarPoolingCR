<td><%= link_to 'Inicio',  usuario_path(@usuario) %></td>
<h2>    <n>Editar Viaje</n>:</h2>
<%= form_for :viaje, url: usuario_viaje_path(@usuario,@viaje), method: :patch do |f| %>
    <p>
      <%= f.label :origen %><br>
      <%= f.text_field :origen%>
    </p>

    <p>
      <%= f.label :latitud_origen %><br>
      <%= f.text_field :latitud_origen %>
    </p>

    <p>
      <%= f.label :longitud_origen %><br>
      <%= f.text_field :longitud_origen %>
    </p>

    <p>
      <%= f.label :destino %><br>
      <%= f.text_field :destino %>
    </p>
    <p>
      <%= f.label :latitud_destino %><br>
      <%= f.text_field :latitud_destino %>
    </p>

    <p>
      <%= f.label :longitud_destino %><br>
      <%= f.text_field :longitud_destino %>
    </p>

    <p>
      <%= f.label :fecha %><br>
      <%= f.text_field :fecha %>
    </p>

    <p>
      <%= f.label :hora %><br>
      <%= f.text_field :hora %>
    </p>


    <p>
      <%= f.label :espacio %><br>
      <%= f.text_field :espacio %>
    </p>

    <p>
      <%= f.label 'Estado' %><br>
      <%= f.text_field :estado %>
    </p>

    <p><%= f.submit 'Editar' %></p>

<% end %>